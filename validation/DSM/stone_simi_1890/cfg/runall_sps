#!/bin/csh
# run a sequence of simulations 1 week 2 months 4 months and annuall
# and use extract.
echo $1
echo "spring week simulation with stone_simi_1890.cfg"
date
&>/dev/null time bps -mode text -file stone_simi_1890.cfg -p spr4_s2 silent
echo "starting extact..."
date
./extract stone_simi_1890_spr4_s2.res
echo "finished spring week"
rm stone_simi_1890_spr4_s2.res
echo "jan-feb simulation with stone_simi_1890.cfg"
date
&>/dev/null time bps -mode text -file stone_simi_1890.cfg -p janfeb4_s2 silent
echo "starting extact..."
date
./extract stone_simi_1890_janfeb4_s2.res
date
echo "finished jan-feb months"
rm stone_simi_1890_janfeb4_s2.res
echo "summer simulation 4tsph s2 with stone_simi_1890.cfg"
&>/dev/null time bps -mode text -file stone_simi_1890.cfg -p summer4_s2 silent
date
echo "starting extact..."
./extract stone_simi_1890_summer4_s2.res
echo "finished summer"
rm stone_simi_1890_summer4_s2.res
echo "annual simulation 4 tsph s2 with stone_simi_1890_jan-feb_s2.cfg"
date
&>/dev/null time bps -mode text -file stone_simi_1890.cfg -p annual4_s2 silent
date
echo "starting extact..."
./extract stone_simi_1890_annual4_s2.res
date
echo "finished annual"
rm stone_simi_1890_annual4_s2.res
echo $1
echo "spring week simulation 4tsph s4 with stone_simi_1890_jan-feb.cfg"
date
&>/dev/null time bps -mode text -file stone_simi_1890.cfg -p spr4_s4 silent
echo "starting extact..."
date
./extract stone_simi_1890_spr4_s4.res
echo "finished spring week"
rm stone_simi_1890_spr4_s4.res
echo "jan-feb simulation 4tsph s4 with stone_simi_1890.cfg"
date
&>/dev/null time bps -mode text -file stone_simi_1890.cfg -p janfeb4 silent
echo "starting extact..."
date
./extract stone_simi_1890_janfeb4.res
date
echo "finished jan-feb months"
rm stone_simi_1890_janfeb4.res
echo "summer simulation 4tsph s4 with stone_simi_1890.cfg"
&>/dev/null time bps -mode text -file stone_simi_1890.cfg -p summer4 silent
date
echo "starting extact..."
./extract stone_simi_1890_summer4.res
echo "finished summer"
rm stone_simi_1890_summer4.res
echo "annual simulation with stone_simi_1890.cfg"
date
&>/dev/null time bps -mode text -file stone_simi_1890.cfg -p annual4_s4 silent
date
echo "starting extact..."
./extract stone_simi_1890_annual4_s4.res
date
rm stone_simi_1890_annual4_s4.res
echo "finished"


